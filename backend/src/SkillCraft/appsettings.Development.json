{
  "Api": {
    "EnableSwagger": true
  },
  "Authentication": {
    "AccessToken": {
      "LifetimeSeconds": 86400
    },
    "EnableBasic": true
  },
  "Database": {
    "ApplyMigrations": true
  },
  "Error": {
    "ExposeDetail": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
