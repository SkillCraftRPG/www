{
  "AllowedHosts": "*",
  "Api": {
    "EnableSwagger": false,
    "Title": "SkillCraftRPG API",
    "Version": "0.10.1"
  },
  "Caching": {
    "ActorLifetime": "00:15:00"
  },
  "Cors": {
    "AllowAnyOrigin": false,
    "AllowedOrigins": [],
    "AllowAnyMethod": false,
    "AllowedMethods": [],
    "AllowAnyHeader": false,
    "AllowedHeaders": [],
    "AllowCredentials": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Permission": {
    "WorldLimit": 3
  },
  "Retry": {
    "Algorithm": "Exponential",
    "Delay": 100,
    "ExponentialBase": 2,
    "RandomVariation": 0,
    "MaximumRetries": 5,
    "MaximumDelay": 0
  }
}
