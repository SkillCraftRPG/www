{
  "AllowedHosts": "*",
  "Api": {
    "EnableSwagger": false,
    "Title": "SkillCraft API",
    "Version": "0.1.0"
  },
  "Authentication": {
    "AccessToken": {
      "Type": "at+jwt",
      "LifetimeSeconds": 300
    },
    "EnableBasic": false
  },
  "Cookies": {
    "RefreshToken": {
      "HttpOnly": true,
      "MaxAge": "7.00:00:00",
      "SameSite": "Strict",
      "Secure": true
    },
    "Session": {
      "SameSite": "Strict"
    }
  },
  "Cors": {
    "AllowAnyOrigin": false,
    "AllowedOrigins": [],
    "AllowAnyMethod": false,
    "AllowedMethods": [],
    "AllowAnyHeader": false,
    "AllowedHeaders": [],
    "AllowCredentials": false
  },
  "Database": {
    "ApplyMigrations": false,
    "Provider": "PostgreSQL"
  },
  "Error": {
    "ExposeDetail": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
